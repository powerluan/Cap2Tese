  )
    )

df_ingressantes["Razao_ingressantes_curso_Federais"] = 
  df_ingressantes$Quantidade_ingressantes_Federais / 
  df_ingressantes$Quantidade_cursos_Federais

df_ingressantes["Razao_ingressantes_curso_Estaduais"] =
  df_ingressantes$Quantidade_ingressantes_Estaduais /
  df_ingressantes$Quantidade_cursos_Estaduais

df_ingressantes["Razao_ingressantes_curso_RFEPCT"] =
  df_ingressantes$Quantidade_ingressantes_RFEPCT /
  df_ingressantes$Quantidade_cursos_RFEPCT

df_ingressantes["Proporcao_ingressantes_feminino_Federais"] =
  df_ingressantes$Quantidade_ingressantes_feminino_Federais /
  df_ingressantes$Quantidade_ingressantes_Federais

df_ingressantes["Proporcao_ingressantes_feminino_Estaduais"] =
  df_ingressantes$Quantidade_ingressantes_feminino_Estaduais /
  df_ingressantes$Quantidade_ingressantes_Estaduais

df_ingressantes["Proporcao_ingressantes_feminino_RFEPCT"] =
  df_ingressantes$Quantidade_ingressantes_feminino_RFEPCT /
  df_ingressantes$Quantidade_ingressantes_RFEPCT

df_ingressantes["Proporcao_ingressantes_nbrancos_Federais"] =
  df_ingressantes$Quantidade_ingressantes_nbrancos_Federais /
  df_ingressantes$Quantidade_ingressantes_Federais

df_ingressantes["Proporcao_ingressantes_nbrancos_Estaduais"] =
  df_ingressantes$Quantidade_ingressantes_nbrancos_Estaduais /
  df_ingressantes$Quantidade_ingressantes_Estaduais

df_ingressantes["Proporcao_ingressantes_nbrancos_RFEPCT"] =
  df_ingressantes$Quantidade_ingressantes_nbrancos_RFEPCT /
  df_ingressantes$Quantidade_ingressantes_RFEPCT

df_ingressantes["Proporcao_ingressantes_reserva_Federais"] =
  df_ingressantes$Quantidade_ingressantes_reserva_Federais /
  df_ingressantes$Quantidade_ingressantes_Federais

df_ingressantes["Proporcao_ingressantes_reserva_Estaduais"] =
  df_ingressantes$Quantidade_ingressantes_reserva_Estaduais /
  df_ingressantes$Quantidade_ingressantes_Estaduais

df_ingressantes["Proporcao_ingressantes_reserva_RFEPCT"] =
  df_ingressantes$Quantidade_ingressantes_reserva_RFEPCT /
  df_ingressantes$Quantidade_ingressantes_RFEPCT

# Gráficos
df_ingressantes %>%
  ggplot(aes(x=Ano, y=Quantidade_ingressantes_TOTAL/1000, group=1)) +
  labs(x="Ano", y="Quantidade de Ingressantes Total (em milhares)") +
  geom_bar(stat="identity", fill="gray") +
  geom_line()

df_ingressantes %>%
  ggplot(aes(x=Ano, y=Quantidade_ingressantes_Federais/1000, group=1)) +
  labs(x="Ano", y="Quantidade de Ingressantes Universidades Federais (em milhares)") +
  geom_bar(stat="identity", fill="red") +
  geom_line()

df_ingressantes %>%
  ggplot(aes(x=Ano, y=Quantidade_ingressantes_Estaduais/1000, group=1)) +
  labs(x="Ano", y="Quantidade de Ingressantes Universidades Estaduais (em milhares)") +
  geom_bar(stat="identity", fill="green") +
  geom_line()

df_ingressantes %>%
  ggplot(aes(x=Ano, y=Quantidade_ingressantes_RFEPCT/1000, group=1)) +
  labs(x="Ano", y="Quantidade de Ingressantes RFEPCT (em milhares)") +
  geom_bar(stat="identity", fill="blue") +
  geom_line()

df_ingressantes %>%
  ggplot(aes(x=Ano, y=Razao_ingressantes_curso_Federais, group=1)) + 
  labs(x="Ano", y="Razão Ingressantes por Cursos - Federais") +
  geom_line() + 
  geom_point() +
  geom_smooth(method = "loess", se = FALSE)

df_ingressantes %>%
