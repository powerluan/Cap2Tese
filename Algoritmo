  ),
  Quantidade_concluintes_RFEPCT = c(
    sum(df_RFEPCT %>% filter(ano == 2009) %>% select(quantidade_concluintes)),
    sum(df_RFEPCT %>% filter(ano == 2010) %>% select(quantidade_concluintes)),
    sum(df_RFEPCT %>% filter(ano == 2011) %>% select(quantidade_concluintes)),
