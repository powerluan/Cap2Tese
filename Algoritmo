# Quantificar concluintes separando por Rede
df_concluintes = data.frame(
  Ano = c('2009','2010','2011','2012','2013','2014','2015','2016','2017',
          '2018','2019','2020','2021'),
  Quantidade_concluintes_Federais = c(
    sum(df_Federais %>% filter(ano == 2009) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2010) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2011) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2012) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2013) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2014) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2015) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2016) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2017) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2018) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2019) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2020) %>% select(quantidade_concluintes)),
    sum(df_Federais %>% filter(ano == 2021) %>% select(quantidade_concluintes))
  ),
  Quantidade_cursos_Federais = c(
    length((df_Federais %>% filter(ano == 2009))$ano),
    length((df_Federais %>% filter(ano == 2010))$ano),
    length((df_Federais %>% filter(ano == 2011))$ano),
    length((df_Federais %>% filter(ano == 2012))$ano),
    length((df_Federais %>% filter(ano == 2013))$ano),
    length((df_Federais %>% filter(ano == 2014))$ano),
    length((df_Federais %>% filter(ano == 2015))$ano),
    length((df_Federais %>% filter(ano == 2016))$ano),
    length((df_Federais %>% filter(ano == 2017))$ano),
    length((df_Federais %>% filter(ano == 2018))$ano),
    length((df_Federais %>% filter(ano == 2019))$ano),
    length((df_Federais %>% filter(ano == 2020))$ano),
    length((df_Federais %>% filter(ano == 2021))$ano)
  ),
  Quantidade_concluintes_Estaduais = c(
    sum(df_Estaduais %>% filter(ano == 2009) %>% select(quantidade_concluintes)),
    sum(df_Estaduais %>% filter(ano == 2010) %>% select(quantidade_concluintes)),
    sum(df_Estaduais %>% filter(ano == 2011) %>% select(quantidade_concluintes)),
