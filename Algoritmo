    length((df_Estaduais %>% filter(ano == 2020))$ano),
    length((df_Estaduais %>% filter(ano == 2021))$ano)
  ),
  Quantidade_ingressantes_RFEPCT = c(
    sum(df_RFEPCT %>% filter(ano == 2009) %>% select(quantidade_ingressantes)),
    sum(df_RFEPCT %>% filter(ano == 2010) %>% select(quantidade_ingressantes)),
    sum(df_RFEPCT %>% filter(ano == 2011) %>% select(quantidade_ingressantes)),
