# Importar pacotes
library(basedosdados)
library(dplyr)
library(ggplot2)
library(gridExtra)
library(scales)
theme_set(theme_bw())

# Definir o projeto no Google Cloud
set_billing_id("censo-da-edu-sup")

# Para carregar o dado direto no R
query = bdplyr("br_inep_censo_educacao_superior.curso")
df = bd_collect(query)

# Filtrar cursos de graduação presenciais das Universidades Federais, Estaduais e RFEPCT
df_novo = df %>%
  select('ano','tipo_organizacao_academica','tipo_organizacao_administrativa',
         'rede','nome_area_geral','tipo_modalidade_ensino','quantidade_ingressantes',
         'quantidade_ingressantes_feminino','quantidade_ingressantes_masculino',
         'quantidade_ingressantes_branca','quantidade_ingressantes_preta',
         'quantidade_ingressantes_parda','quantidade_ingressantes_amarela',
         'quantidade_ingressantes_indigena','quantidade_matriculas',
         'quantidade_matriculas_feminino','quantidade_matriculas_masculino',
         'quantidade_matriculas_branca','quantidade_matriculas_preta',
         'quantidade_matriculas_parda','quantidade_matriculas_amarela',
         'quantidade_matriculas_indigena','quantidade_concluintes',
         'quantidade_concluintes_feminino','quantidade_concluintes_masculino',
         'quantidade_concluintes_branca','quantidade_concluintes_preta',
         'quantidade_concluintes_parda','quantidade_concluintes_amarela',
         'quantidade_concluintes_indigena','quantidade_ingressantes_reserva_vaga',
         'quantidade_matriculas_reserva_vaga','quantidade_concluintes_reserva_vaga') %>%
  filter(tipo_modalidade_ensino == 1, 
         tipo_organizacao_administrativa == 1 | tipo_organizacao_administrativa == 2, 
         tipo_organizacao_academica == 1 | tipo_organizacao_academica == 4 | 
           tipo_organizacao_academica == 5) %>%
  mutate(tipo_organizacao_academica = as.numeric(tipo_organizacao_academica)) %>%
  mutate(tipo_organizacao_administrativa = as.numeric(tipo_organizacao_administrativa)) %>%
  mutate(rede = tipo_organizacao_academica + tipo_organizacao_administrativa) %>%
  mutate(rede = ifelse(rede == 2, "Universidades Federais",
                       ifelse(rede == 3, "Universidades Estaduais","RFEPCT")))

# Quantidade de cursos
df_novo %>%
  ggplot(mapping=aes(x=ano, fill=rede)) +
  labs(x="Ano", y="Quantidade de cursos") +
  geom_bar(position="dodge")

# Carregar banco de dados para Federais, Estaduais e RFEPCT
df_Federais = df_novo %>%
  select('ano','tipo_organizacao_academica','tipo_organizacao_administrativa',
         'rede','nome_area_geral','tipo_modalidade_ensino','quantidade_ingressantes',
         'quantidade_ingressantes_feminino','quantidade_ingressantes_masculino',
         'quantidade_ingressantes_branca','quantidade_ingressantes_preta',
         'quantidade_ingressantes_parda','quantidade_ingressantes_amarela',
         'quantidade_ingressantes_indigena','quantidade_matriculas',
         'quantidade_matriculas_feminino','quantidade_matriculas_masculino',
         'quantidade_matriculas_branca','quantidade_matriculas_preta',
         'quantidade_matriculas_parda','quantidade_matriculas_amarela',
         'quantidade_matriculas_indigena','quantidade_concluintes',
         'quantidade_concluintes_feminino','quantidade_concluintes_masculino',
         'quantidade_concluintes_branca','quantidade_concluintes_preta',
         'quantidade_concluintes_parda','quantidade_concluintes_amarela',
         'quantidade_concluintes_indigena','quantidade_ingressantes_reserva_vaga',
         'quantidade_matriculas_reserva_vaga','quantidade_concluintes_reserva_vaga') %>%
  filter(tipo_organizacao_administrativa == 1, tipo_organizacao_academica == 1)

df_Estaduais = df_novo %>%
  select('ano','tipo_organizacao_academica','tipo_organizacao_administrativa',
         'rede','nome_area_geral','tipo_modalidade_ensino','quantidade_ingressantes',
         'quantidade_ingressantes_feminino','quantidade_ingressantes_masculino',
         'quantidade_ingressantes_branca','quantidade_ingressantes_preta',
         'quantidade_ingressantes_parda','quantidade_ingressantes_amarela',
         'quantidade_ingressantes_indigena','quantidade_matriculas',
         'quantidade_matriculas_feminino','quantidade_matriculas_masculino',
         'quantidade_matriculas_branca','quantidade_matriculas_preta',
         'quantidade_matriculas_parda','quantidade_matriculas_amarela',
         'quantidade_matriculas_indigena','quantidade_concluintes',
         'quantidade_concluintes_feminino','quantidade_concluintes_masculino',
         'quantidade_concluintes_branca','quantidade_concluintes_preta',
         'quantidade_concluintes_parda','quantidade_concluintes_amarela',
         'quantidade_concluintes_indigena','quantidade_ingressantes_reserva_vaga',
         'quantidade_matriculas_reserva_vaga','quantidade_concluintes_reserva_vaga') %>%
  filter(tipo_organizacao_administrativa == 2, tipo_organizacao_academica == 1)

df_RFEPCT = df_novo %>%
  select('ano','tipo_organizacao_academica','tipo_organizacao_administrativa',
         'rede','nome_area_geral','tipo_modalidade_ensino','quantidade_ingressantes',
         'quantidade_ingressantes_feminino','quantidade_ingressantes_masculino',
         'quantidade_ingressantes_branca','quantidade_ingressantes_preta',
         'quantidade_ingressantes_parda','quantidade_ingressantes_amarela',
         'quantidade_ingressantes_indigena','quantidade_matriculas',
         'quantidade_matriculas_feminino','quantidade_matriculas_masculino',
         'quantidade_matriculas_branca','quantidade_matriculas_preta',
         'quantidade_matriculas_parda','quantidade_matriculas_amarela',
         'quantidade_matriculas_indigena','quantidade_concluintes',
         'quantidade_concluintes_feminino','quantidade_concluintes_masculino',
         'quantidade_concluintes_branca','quantidade_concluintes_preta',
         'quantidade_concluintes_parda','quantidade_concluintes_amarela',
         'quantidade_concluintes_indigena','quantidade_ingressantes_reserva_vaga',
         'quantidade_matriculas_reserva_vaga','quantidade_concluintes_reserva_vaga') %>%
  filter(tipo_organizacao_administrativa == 1, tipo_organizacao_academica == 4 | 
           tipo_organizacao_academica == 5)

####################### - INGRESSANTES - ##################################

# Quantificar ingressantes separando por Rede
df_ingressantes = data.frame(
  Ano = c('2009','2010','2011','2012','2013','2014','2015','2016','2017',
          '2018','2019','2020','2021'),
  Quantidade_ingressantes_Federais = c(
    sum(df_Federais %>% filter(ano == 2009) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2010) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2011) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2012) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2013) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2014) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2015) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2016) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2017) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2018) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2019) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2020) %>% select(quantidade_ingressantes)),
    sum(df_Federais %>% filter(ano == 2021) %>% select(quantidade_ingressantes))
    ),
  Quantidade_cursos_Federais = c(
    length((df_Federais %>% filter(ano == 2009))$ano),
    length((df_Federais %>% filter(ano == 2010))$ano),
