                                                       quantidade_ingressantes_amarela,
                                                       quantidade_ingressantes_indigena)),
    sum(df_Federais %>% filter(ano == 2018) %>% select(quantidade_ingressantes_preta,
                                                       quantidade_ingressantes_parda,
                                                       quantidade_ingressantes_amarela,
                                                       quantidade_ingressantes_indigena)),
    sum(df_Federais %>% filter(ano == 2019) %>% select(quantidade_ingressantes_preta,
                                                       quantidade_ingressantes_parda,
                                                       quantidade_ingressantes_amarela,
                                                       quantidade_ingressantes_indigena)),
    sum(df_Federais %>% filter(ano == 2020) %>% select(quantidade_ingressantes_preta,
                                                       quantidade_ingressantes_parda,
                                                       quantidade_ingressantes_amarela,
                                                       quantidade_ingressantes_indigena)),
    sum(df_Federais %>% filter(ano == 2021) %>% select(quantidade_ingressantes_preta,
                                                       quantidade_ingressantes_parda,
                                                       quantidade_ingressantes_amarela,
                                                       quantidade_ingressantes_indigena))
  ),
  Quantidade_ingressantes_nbrancos_Estaduais = c(
    sum(df_Estaduais %>% filter(ano == 2009) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2010) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2011) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2012) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2013) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2014) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2015) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2016) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2017) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2018) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2019) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2020) %>% select(quantidade_ingressantes_preta,
                                                        quantidade_ingressantes_parda,
                                                        quantidade_ingressantes_amarela,
                                                        quantidade_ingressantes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2021) %>% select(quantidade_ingressantes_preta,
