                                                     quantidade_concluintes_parda,
                                                     quantidade_concluintes_amarela,
                                                     quantidade_concluintes_indigena))
  )
)

df_concluintes["Razao_concluintes_curso_Federais"] = 
  df_concluintes$Quantidade_concluintes_Federais / 
  df_concluintes$Quantidade_cursos_Federais

df_concluintes["Razao_concluintes_curso_Estaduais"] =
  df_concluintes$Quantidade_concluintes_Estaduais /
  df_concluintes$Quantidade_cursos_Estaduais

df_concluintes["Razao_concluintes_curso_RFEPCT"] =
  df_concluintes$Quantidade_concluintes_RFEPCT /
  df_concluintes$Quantidade_cursos_RFEPCT

df_concluintes["Proporcao_concluintes_feminino_Federais"] =
  df_concluintes$Quantidade_concluintes_feminino_Federais /
  df_concluintes$Quantidade_concluintes_Federais

df_concluintes["Proporcao_concluintes_feminino_Estaduais"] =
  df_concluintes$Quantidade_concluintes_feminino_Estaduais /
  df_concluintes$Quantidade_concluintes_Estaduais

df_concluintes["Proporcao_concluintes_feminino_RFEPCT"] =
  df_concluintes$Quantidade_concluintes_feminino_RFEPCT /
  df_concluintes$Quantidade_concluintes_RFEPCT

df_concluintes["Proporcao_concluintes_nbrancos_Federais"] =
  df_concluintes$Quantidade_concluintes_nbrancos_Federais /
  df_concluintes$Quantidade_concluintes_Federais

df_concluintes["Proporcao_concluintes_nbrancos_Estaduais"] =
  df_concluintes$Quantidade_concluintes_nbrancos_Estaduais /
  df_concluintes$Quantidade_concluintes_Estaduais

df_concluintes["Proporcao_concluintes_nbrancos_RFEPCT"] =
  df_concluintes$Quantidade_concluintes_nbrancos_RFEPCT /
  df_concluintes$Quantidade_concluintes_RFEPCT

df_concluintes["Proporcao_concluintes_reserva_Federais"] =
  df_concluintes$Quantidade_concluintes_reserva_Federais /
  df_concluintes$Quantidade_concluintes_Federais

df_concluintes["Proporcao_concluintes_reserva_Estaduais"] =
  df_concluintes$Quantidade_concluintes_reserva_Estaduais /
  df_concluintes$Quantidade_concluintes_Estaduais

df_concluintes["Proporcao_concluintes_reserva_RFEPCT"] =
  df_concluintes$Quantidade_concluintes_reserva_RFEPCT /
  df_concluintes$Quantidade_concluintes_RFEPCT

# Gráficos
df_concluintes %>%
  ggplot(aes(x=Ano, y=Quantidade_concluintes_TOTAL/1000, group=1)) +
  labs(x="Ano", y="Quantidade de Concluintes Total (em milhares)") +
  geom_bar(stat="identity", fill="gray") +
  geom_line()

df_concluintes %>%
  ggplot(aes(x=Ano, y=Quantidade_concluintes_Federais/1000, group=1)) +
  labs(x="Ano", y="Quantidade de Concluintes Universidades Federais (em milhares)") +
  geom_bar(stat="identity", fill="red") +
  geom_line()

df_concluintes %>%
  ggplot(aes(x=Ano, y=Quantidade_concluintes_Estaduais/1000, group=1)) +
  labs(x="Ano", y="Quantidade de Concluintes Universidades Estaduais (em milhares)") +
  geom_bar(stat="identity", fill="green") +
  geom_line()

df_concluintes %>%
  ggplot(aes(x=Ano, y=Quantidade_concluintes_RFEPCT/1000, group=1)) +
  labs(x="Ano", y="Quantidade de Concluintes RFEPCT (em milhares)") +
  geom_bar(stat="identity", fill="blue") +
  geom_line()

df_concluintes %>%
  ggplot(aes(x=Ano, y=Razao_concluintes_curso_Federais, group=1)) + 
  labs(x="Ano", y="Razão Concluintes por Cursos - Federais") +
  geom_line() + 
  geom_point() +
  geom_smooth(method = "loess", se = FALSE)

df_concluintes %>%
  ggplot(aes(x=Ano, y=Razao_concluintes_curso_Estaduais, group=1)) + 
  labs(x="Ano", y="Razão Concluintes por Cursos - Estaduais") +
  geom_line() + 
  geom_point() +
  geom_smooth(method = "loess", se = FALSE)

df_concluintes %>%
