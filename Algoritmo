    sum(df_Estaduais %>% filter(ano == 2011) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2012) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2013) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2014) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2015) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2016) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2017) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2018) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2019) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2020) %>% select(quantidade_concluintes_feminino)),
    sum(df_Estaduais %>% filter(ano == 2021) %>% select(quantidade_concluintes_feminino))
  ),
  Quantidade_concluintes_feminino_RFEPCT = c(
    sum(df_RFEPCT %>% filter(ano == 2009) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2010) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2011) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2012) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2013) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2014) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2015) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2016) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2017) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2018) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2019) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2020) %>% select(quantidade_concluintes_feminino)),
    sum(df_RFEPCT %>% filter(ano == 2021) %>% select(quantidade_concluintes_feminino))
  ),
  Quantidade_concluintes_reserva_Federais = c(
    sum(df_Federais %>% filter(ano == 2009) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2010) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2011) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2012) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2013) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2014) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2015) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2016) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2017) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2018) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2019) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2020) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Federais %>% filter(ano == 2021) %>% select(quantidade_concluintes_reserva_vaga))
  ),
  Quantidade_concluintes_reserva_Estaduais = c(
    sum(df_Estaduais %>% filter(ano == 2009) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2010) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2011) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2012) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2013) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2014) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2015) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2016) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2017) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2018) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2019) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2020) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_Estaduais %>% filter(ano == 2021) %>% select(quantidade_concluintes_reserva_vaga))
  ),
  Quantidade_concluintes_reserva_RFEPCT = c(
    sum(df_RFEPCT %>% filter(ano == 2009) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2010) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2011) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2012) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2013) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2014) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2015) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2016) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2017) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2018) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2019) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2020) %>% select(quantidade_concluintes_reserva_vaga)),
    sum(df_RFEPCT %>% filter(ano == 2021) %>% select(quantidade_concluintes_reserva_vaga))
  ),
  Quantidade_concluintes_nbrancos_Federais = c(
    sum(df_Federais %>% filter(ano == 2009) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2010) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2011) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2012) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2013) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2014) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2015) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2016) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2017) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2018) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2019) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2020) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena)),
    sum(df_Federais %>% filter(ano == 2021) %>% select(quantidade_concluintes_preta,
                                                       quantidade_concluintes_parda,
                                                       quantidade_concluintes_amarela,
                                                       quantidade_concluintes_indigena))
  ),
  Quantidade_concluintes_nbrancos_Estaduais = c(
    sum(df_Estaduais %>% filter(ano == 2009) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2010) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2011) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2012) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2013) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2014) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2015) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2016) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2017) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2018) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2019) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2020) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena)),
    sum(df_Estaduais %>% filter(ano == 2021) %>% select(quantidade_concluintes_preta,
                                                        quantidade_concluintes_parda,
                                                        quantidade_concluintes_amarela,
                                                        quantidade_concluintes_indigena))
  ),
  Quantidade_concluintes_nbrancos_RFEPCT = c(
    sum(df_RFEPCT %>% filter(ano == 2009) %>% select(quantidade_concluintes_preta,
                                                     quantidade_concluintes_parda,
                                                     quantidade_concluintes_amarela,
                                                     quantidade_concluintes_indigena)),
    sum(df_RFEPCT %>% filter(ano == 2010) %>% select(quantidade_concluintes_preta,
                                                     quantidade_concluintes_parda,
                                                     quantidade_concluintes_amarela,
                                                     quantidade_concluintes_indigena)),
    sum(df_RFEPCT %>% filter(ano == 2011) %>% select(quantidade_concluintes_preta,
                                                     quantidade_concluintes_parda,
                                                     quantidade_concluintes_amarela,
                                                     quantidade_concluintes_indigena)),
    sum(df_RFEPCT %>% filter(ano == 2012) %>% select(quantidade_concluintes_preta,
                                                     quantidade_concluintes_parda,
                                                     quantidade_concluintes_amarela,
                                                     quantidade_concluintes_indigena)),
    sum(df_RFEPCT %>% filter(ano == 2013) %>% select(quantidade_concluintes_preta,
                                                     quantidade_concluintes_parda,
                                                     quantidade_concluintes_amarela,
                                                     quantidade_concluintes_indigena)),
