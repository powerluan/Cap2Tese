  Quantidade_cursos_RFEPCT = c(
    length((df_RFEPCT %>% filter(ano == 2009))$ano),
    length((df_RFEPCT %>% filter(ano == 2010))$ano),
    length((df_RFEPCT %>% filter(ano == 2011))$ano),
    length((df_RFEPCT %>% filter(ano == 2012))$ano),
